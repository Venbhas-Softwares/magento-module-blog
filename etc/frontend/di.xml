<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\Framework\App\RouterList">
        <arguments>
            <argument name="routerList" xsi:type="array">
                <item name="article" xsi:type="array">
                    <item name="class" xsi:type="string">Venbhas\Article\Controller\Router</item>
                    <item name="disable" xsi:type="boolean">false</item>
                    <!-- Run after urlrewrite (20) but before standard (30) so standard can match article/comment/post when we return null -->
                    <item name="sortOrder" xsi:type="string">25</item>
                </item>
            </argument>
        </arguments>
    </type>
</config>
